{
   "action": {
      "default_title": "Start my extension"
   },
   "background": {
      "service_worker": "./build/background.js"
   },
   "description": "Bring your own website into Modified Framer with a few clicks. Simply copy and paste web elements and edit them directly in Framer.",
   "host_permissions": [ "https://*/*" ],
   "icons": {
      "128": "icons/default/icon128.png",
      "16": "icons/default/icon16.png",
      "32": "icons/default/icon32.png",
      "48": "icons/default/icon48.png"
   },
   "manifest_version": 3,
   "name": "HTML to Framer Modified",
   "permissions": [ "activeTab", "scripting", "webNavigation" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.6"
}
